{
	"name": "wolfsblvt/highlightunreadposts",
	"type": "phpbb-extension",
	"description": "Highlights all unread posts in a topic. Color can be chosen in ACP.",
	"homepage": "http://pinkes-forum.de/dev/find.php?ext=highlightunreadposts",
	"version": "1.0.2",
	"time": "2015-04-23",
	"keywords": ["phpbb", "extension", "highlightunreadposts", "viewtopic", "unread"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Clemens Husung (Wolfsblvt)",
			"homepage": "http://www.pinkes-forum.de/",
			"email": "wolfsblvt@gmx.de",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Highlight Unread Posts",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.*@dev"
		},
		"version-check": {
			"host": "pinkes-forum.de",
			"directory": "/dev/highlightunreadposts",
			"filename": "version.json"
		}
	}
}
